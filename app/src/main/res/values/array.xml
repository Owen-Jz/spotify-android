<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Explorer Menu -->
    <array name="explorer_menu_items">
        <item>@array/lists</item>
        <item>@array/new_releases</item>
        <item>@array/discover</item>
        <item>@array/concerts</item>
    </array>

    <array name="lists">
        <item>@id/lists</item>
        <item>fa_line_chart</item>
        <item>@string/explorer_menu_lists</item>
    </array>

    <array name="new_releases">
        <item>@id/new_releases</item>
        <item>fa_calendar_o</item>
        <item>@string/explorer_menu_new_releases</item>
    </array>

    <array name="discover">
        <item>@id/discover</item>
        <item>fa_calendar_o</item>
        <item>@string/explorer_menu_discover</item>
    </array>

    <array name="concerts">
        <item>@id/concerts</item>
        <item>fa_ticket</item>
        <item>@string/explorer_menu_concerts</item>
    </array>

    <!-- Your library menu -->
    <array name="your_library_menu">
        <item>@array/playlist</item>
        <item>@array/stations</item>
        <item>@array/songs</item>
        <item>@array/albums</item>
        <item>@array/artist</item>
    </array>

    <array name="playlist">
        <item>@id/library_playlist</item>
        <item>fa_music</item>
        <item>@string/your_library_menu_playlist</item>
    </array>

    <array name="stations">
        <item>@id/library_stations</item>
        <item>fa_music</item>
        <item>@string/your_library_menu_stations</item>
    </array>

    <array name="songs">
        <item>@id/library_songs</item>
        <item>fa_headphones</item>
        <item>@string/your_library_menu_song</item>
    </array>

    <array name="albums">
        <item>@id/library_albums</item>
        <item>fa_music</item>
        <item>@string/your_library_menu_albums</item>
    </array>

    <array name="artist">
        <item>@id/library_artists</item>
        <item>fa_music</item>
        <item>@string/your_library_menu_artists</item>
    </array>




    <!-- App Services -->
    <string-array name="app_services">
        <item>UserService</item>
        <item>CategoriesService</item>
        <item>PlaylistsService</item>
        <item>AlbumsService</item>
        <item>ArtistsService</item>
        <item>TracksServices</item>
    </string-array>



</resources>